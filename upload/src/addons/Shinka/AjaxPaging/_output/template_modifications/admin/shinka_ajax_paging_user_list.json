{
    "template": "user_list",
    "description": "Adds animate and replace to page nav params.",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "params=\"{{ {'criteria': $criteria, 'order': $order, 'direction': $direction} }}\"",
    "replace": "params=\"{{ {'criteria': $criteria, 'order': $order, 'direction': $direction, 'ajax': true, 'replace': '.block with .block'} }}\""
}